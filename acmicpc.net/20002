#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int memo[301][301];

int main() {
    int n;

    cin >> n;

    //0 0 0 0
    //0 
    //0
    //0
    for (int i = 0; i <= 300; i++) {
        memo[0][i] = 0;
        memo[i][0] = 0;
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            int t;
            cin >> t;
            memo[i][j] = t + memo[i-1][j] + memo[i][j-1] - memo[i-1][j-1];
        }
    }
    
    for(

}
